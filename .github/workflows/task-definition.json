{
  "family": "fargate",
  "containerDefinitions": [
    {
      "name": "cougarcs-bot-dev",
      "image": "https://056781185616.dkr.ecr.us-east-2.amazonaws.com/cougarcs-bot:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "bot-dev-container",
          "awslogs-region": "us-east-2",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "bot-dev"
        }
      },
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:056781185616:secret:bot-dev-env-61dWzO:TOKEN"
        },
        {
          "name": "GUILD_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:056781185616:secret:bot-dev-env-61dWzO:GUILD_ID"
        },
        {
          "name": "CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:056781185616:secret:bot-dev-env-61dWzO:CLIENT_ID"
        },
        {
          "name": "SUPABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:056781185616:secret:bot-dev-env-61dWzO:SUPABASE_URL"
        },
        {
          "name": "SUPABASE_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:056781185616:secret:bot-dev-env-61dWzO:SUPABASE_KEY"
        }
      ]
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::056781185616:role/ecsTaskExecutionRole",
  "memory": "512"
}
